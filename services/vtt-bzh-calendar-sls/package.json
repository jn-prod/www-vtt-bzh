  "engines": {
    "node": "16"
  },
  "dependencies": {
    "@swan-io/boxed": "^0.11.1",
    "axios": "latest",
    "common": "workspace:*",
    "linkedom": "0",
    "mongodb": "latest",
    "text-converter": "workspace:*",
    "typescript": "^4.8.3",
    "utils": "workspace:*"
  },
  "devDependencies": {
    "@types/axios": "latest",
    "@types/iconv-lite": "latest",
    "@types/jest": "latest",
    "esbuild": "0",
    "esbuild-jest": "latest",
    "eslint-config-lambda": "workspace:*",
    "jest": "latest",
    "prettier-custom": "workspace:*",
    "serverless": "3",
    "serverless-dotenv-plugin": "latest",
    "serverless-esbuild": "1",
    "serverless-offline": "12",
