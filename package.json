    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.3",
    "turbo": "^1.10.13"
  },
  "manypkg": {
    "ignoredRules": [
      "INTERNAL_MISMATCH"
    ]
  },
  "lint-staged": {
    "*.{js,ts,tsx,vue}": "eslint --fix --ignore-path .gitignore",
    "*.{json,yaml,yml,md,mdx}": "prettier --write"
